{
  "camera": {
    "captureResolution": "1080p",
    "pictureResolution": "1080p"
  },
  "flash": {
    "mode": "manual",
    "alignment": "top_right"
  },
  "viewPlugin": {
    "plugin": {
      "id": "METER_PLUGIN",
      "meterPlugin": {
        "scanMode": "DIGITAL_METER_MULTIFIELDS"
      }
    },
    "cutoutConfig": {
      "style": "rect",
      "alignment": "top_half",
      "strokeWidth": 2,
      "strokeColor": "FFFFFF",
      "cornerRadius": 4,
      "outerColor": "000000",
      "outerAlpha": 0.5,
      "feedbackStrokeColor": "0099FF"
    },
    "scanFeedback": {
      "style": "CONTOUR_RECT",
      "strokeColor": "0099FF",
      "fillColor": "220099FF",
      "blinkOnResult": true,
      "beepOnResult": true,
      "vibrateOnResult": true
    },
    "cancelOnResult": false,
    "delayStartScanTime": 1000
  }
}